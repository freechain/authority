{
  "name": "ethnamed",
  "version": "0.0.1",
  "description": "This dapp helps register names in Ethnamed Registry",
  "main": "server.js",
  "dependencies": {
    "async": "^2.6.0",
    "bignumber.js": "^6.0.0",
    "body-parser": "^1.18.2",
    "decomment": "^0.9.1",
    "ethereumjs-tx": "^1.3.4",
    "mobx": "^4.1.0",
    "mobx-react": "^5.0.0",
    "prelude-ls": "^1.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "require-ls": "0.0.8",
    "solc": "^0.4.21",
    "web3": "^0.19.0"
  },
  "scripts": {
    "quick-start": "npm run install && npm run compile && node server.js",
    "start": "node server.js",
    "start-blockchain" : "cd ./blockchain && sh /ganache-c9.sh && cd ../",
    "deploy" : "cd ./blockchain && lsc ./deploy.ls && cd ../",
    "install": "npm i && npm i -g livescript lsxc forever ganache-cli",
    "compile": "cd ./dapp && lsxc -t index.html -hpskbfc  client.ls && cd ../ && lsc -c server.ls",
    "compile-watch": "cd ./dapp && lsxc -t index.html -hpskbfcw client.ls && cd ../"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethnamed/registrant-dapp.git"
  },
